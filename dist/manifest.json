{
  "name": "PSHELPER",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "PS Helper for extension",
  "icons": {
    "128": "logo.png"
  },
  "permissions": [],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html",
    "default_title": "PSHELPER"
  },
  "options_page": "index.html"
  }