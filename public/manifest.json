{
  "name": "PSHELPER",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "PS Helper for extension",
  "icons": {
    "128": "logo.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html",
    "default_title": "PSHELPER"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["contentScript.js"]
    }
  ],
  "permissions": [
    "tabs",
    "<all_urls>",
    "storage",
    "activeTab"
  ],
  "options_page": "index.html"
  }